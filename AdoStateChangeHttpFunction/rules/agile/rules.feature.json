{
    "type": "Feature",
    "rules": [
      {
        "ifChildState": "Active",
        "notParentStates": [ "Active", "Resolved" ],
        "setParentStateTo": "Active",
        "allChildren": false
      },
      {
        "ifChildState": "New",
        "notParentStates": [ "Active", "New" ],
        "setParentStateTo": "Active",
        "allChildren": false
      },
      {
        "ifChildState": "Resolved",
        "notParentStates": [],
        "setParentStateTo": "Resolved",
        "allChildren": true
      },
      {
        "ifChildState": "Closed",
        "notParentStates": [],
        "setParentStateTo": "Closed",
        "allChildren": true
      }
    ]
  }